{
  "name": "ccinn/kafka-swoole",
  "description": "Kafka client for php",
  "type": "library",
  "keywords": [
    "Client",
    "Kafka"
  ],
  "config": {
    "platform": {
      "php": "7.1.12"
    },
    "preferred-install": "dist",
    "sort-packages": true
  },
  "require": {
    "ext-json": "*",
    "php": "^7.1",
    "symfony/console": "^4.3",
    "symfony/dotenv": "^4.3",
    "symfony/event-dispatcher": "^4.3",
    "symfony/translation": "^4.3",
    "symfony/yaml": "^4.3"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app",
      "Kafka\\": "src",
      "\\": "app"
    },
    "files": [
      "src/Support/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "KafkaTest\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^7.5"
  }
}
